{
  "name": "@uni-helper/vite-plugin-uni-middleware",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.16.1",
  "description": "Use route middleware in uni-app with Vite.",
  "author": "KeJun",
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/vite-plugin-uni-middleware#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/vite-plugin-uni-middleware.git"
  },
  "bugs": "https://github.com/uni-helper/vite-plugin-uni-middleware/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "require": "./dist/runtime/index.cjs",
      "import": "./dist/runtime/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ],
      "runtime": [
        "./dist/runtime/*",
        "./dist/runtime/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "play": "nr -C playground dev:h5",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish --access public"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.0.19",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "jsonc-parser": "^3.2.0",
    "scule": "^1.0.0"
  }
}
